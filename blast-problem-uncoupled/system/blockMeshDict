FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

scale               1;  

// Dimensions: 
// Block 1: 7.4 (x) by 4m (y)
// Block 2: 7.4 (x) by 2m (y) - Starting from y = 4m
// Block 3: 0.3 (x) by 2m (y) - Starting from x = 7.4m and y = 4m
// Block 4: 2.3 (x) by 2m (y) - Starting from x = 7.7m and y = 4 m
// Block 5: 2.3 (x) by 4m (y) - Starting from x = 7.7m

vertices
(
    // Block 1: Lower Face (z = 0)
    (0 0 0)         // 0
    (7.4 0 0)       // 1
    (7.4 4 0)       // 2
    (0 4 0)         // 3

    // Block 1: Upper Face (z = 0.1)
    (0 0 0.1)      // 4
    (7.4 0 0.1)    // 5
    (7.4 4 0.1)    // 6
    (0 4 0.1)      // 7

    // Block 2: Lower Face Missing Vertexes (z = 0)
    (0 6 0)        // 8
    (7.4 6 0)      // 9

    // Block 2: Upper Face Missing Vertexes (z = 0.1)
    (0 6 0.1)      // 10
    (7.4 6 0.1)    // 11

    // Block 3: Lower Face Missing Vertexes (z = 0)
    (7.7 4 0)      // 12
    (7.7 6 0)      // 13

    // Block 3: Upper Face Missing Vertexes (z = 0.1)
    (7.7 4 0.1)    // 14
    (7.7 6 0.1)    // 15

    // Block 4: Lower Face Missing Vertexes (z = 0)
    (10 4 0)       // 16
    (10 6 0)       // 17

    // Block 4: Upper Face Missing Vertexes (z = 0.1)
    (10 4 0.1)     // 18
    (10 6 0.1)     // 19

    // Block 5: Lower Face Missing Vertexes (z = 0)
    (10 0 0)       // 20
    (7.7 0 0)      // 21

    // Block 5: Upper Face Missing Vertexes (z = 0.1)
    (10 0 0.1)     // 22
    (7.7 0 0.1)    // 23

);


// Default Cell Dimension: 0.1 x 0.1.
// To refine the mesh, edit the 'resolution_factor' down below.

 
#include "meshProperties"

blocks
(
    // hex (v0 v1 v2 v3 v4 v5 v6 v7) (n_x n_y n_z) simpleGrading (1 1 1)

    // Block 1

    hex (0 1 2 3 4 5 6 7)           
    ($block1_x $block1_y 1)                       // Number of cells in (x, y, z) directions
    simpleGrading (1 1 1)           // Simple uniform grading

    // Block 2

    hex (3 2 9 8 7 6 11 10)
    ($block2_x $block2_y 1)
    simpleGrading (1 1 1)

    // Block 3

    hex (2 12 13 9 6 14 15 11)
    ($block3_x $block3_y 1)
    simpleGrading (1 1 1)

     // Block 4

    hex (12 16 17 13 14 18 19 15)
    ($block4_x $block4_y 1)
    simpleGrading (1 1 1)

    // Block 5

    hex (21 20 16 12 23 22 18 14)
    ($block5_x $block5_y 1)
    simpleGrading (1 1 1)

);

boundary               // Keyword
(
    outerContour       // Patch name
    {
        type wall;    // patch type for patch 0
        faces
        (
            (0 1 5 4) // Bottom Face - Block 1
            (0 4 7 3)  // Left Face Block 1
            (3 7 10 8) // Left Face Block 2
            (9 8 10 11) // Top Face - Block 2
            (13 9 11 15) // Top Face - Block 3
            (17 13 15 19) // Top Face - Block 4
            (16 17 19 18) // Right Face Block 4
            (20 16 18 22) // Right Face Block 5
            (20 22 23 21) // Bottom Face - Block 5
        );
    }

    structureWall             // patch name
    {
        type wall;    // patch type for patch 1
        faces
        (
            (1 2 6 5) // Right Face - Block 1
            (2 12 14 6) // Bottom Face - Block 3
            (14 12 21 23) // Left Face - Block 5
        );
    }
    
    frontAndBack
    {
        type empty;
        faces
        (
            // Block 1
            (0 3 2 1)
            (4 5 6 7)
            
            // Block 2
            (3 8 9 2)
            (7 6 11 10)

            // Block 3
            (2 9 13 12)
            (6 14 15 11)

            // Block 4
            (12 13 17 16)
            (14 18 19 15)

            // Block 5
            (18 14 23 22)
            (12 16 20 21)
        );
    }
);

